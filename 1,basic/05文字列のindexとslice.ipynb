     "text": [
      "p\n",
      "y\n",
      "n\n",
      "tho\n",
      "pytho\n",
      "thon\n",
      "pto\n"
     ]
    }
   ],
   "source": [
    "word = \"python\"\n",
    "\n",
    "print(word[0])\n",
    "print(word[1])\n",
    "print(word[-1])\n",
    "\n",
    "print(word[2:5])\n",
    "print(word[:5])\n",
    "print(word[2:])\n",
    "\n",
    "print(word[::2])"
   ]
  },
